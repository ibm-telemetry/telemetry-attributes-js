{
  "name": "@ibm/telemetry-attributes-js",
  "description": "IBM Telemetry attribute definitions for JS code",
  "version": "4.0.0",
  "license": "Apache-2.0",
  "author": "IBM",
  "keywords": [
    "ibm",
    "telemetry",
    "opentelemetry"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ibm-telemetry/telemetry-attributes-js#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ibm-telemetry/telemetry-attributes-js.git"
  },
  "bugs": {
